{
  "name": "mock",
  "version": "2.0",
  "description": "基于Vite的chrome插件V3 Demo",
  "manifest_version": 3,
  "background": {
    "service_worker": "background/index.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; frame-ancestors 'none';"
  },
  "externally_connectable": {
    "ids": [
      "*"
    ]
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": ["*://*/*"],
  "web_accessible_resources": [
    {
      "resources": [
        "/favicon.ico"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "content/index.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/popup/index.html",
    "default_icon": {
      "16": "icon/icon-16.png",
      "32": "icon/icon-32.png",
      "48": "icon/icon-48.png",
      "64": "icon/icon-64.png",
      "128": "icon/icon-128.png"
    },
    "default_title": "Vue CRX MV3"
  },
  "icons": {
    "16": "icon/icon-16.png",
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "64": "icon/icon-64.png",
    "128": "icon/icon-128.png"
  }
}