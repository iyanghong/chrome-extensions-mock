{
  "name": "Mock Data",
  "version": "1.0",
  "manifest_version": 3,
  "description": "模拟数据",
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "html/index.html"
  },
  "options_page": "html/options.html",
  "permissions": [
    "contextMenus",
    "scripting",
    "tabs",
    "history",
    "declarativeContent",
    "activeTab",
    "storage",
    "notifications"
  ],
  "content_scripts": [
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-48.png"
  }
}